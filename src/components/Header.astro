---
interface Props {
    title: string;
}

const { title } = Astro.props
---

<header class="flex flex-row justify-center items-center">
    <div class="h-0 border border-secondary mr-12 w-60"></div>
    <h2 class="text-primary text-center font-sans text-4xl font-semibold">{title}</h2>
    <div class="h-0 border border-secondary ml-12 w-60"></div>
</header>